{
    "name": "synaq/curl-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 wrapper for Curl",
    "keywords": ["API","cURL","Symfony2"],
    "homepage": "https://github.com/synaq/SynaqCurlBundle",
    "authors": [
        {
            "name": "Nic Parry",
            "email": "nicjparry@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "symfony/framework-bundle": ">=2.1"
    },
    "require-dev": {
        "symfony/yaml": "2.1.*",
        "symfony/validator": "2.1.*",
        "phpunit/phpunit": "dev-master"
    },
    "autoload": {
        "psr-0": { "Synaq\\CurlBundle": "" }
    },
    "target-dir": "Synaq/CurlBundle"
}
